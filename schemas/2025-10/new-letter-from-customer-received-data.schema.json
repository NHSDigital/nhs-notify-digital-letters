{
  "$id": "https://notify.nhs.uk/schemas/events/digital-letters/2025-10/new-letter-from-customer-received-data.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": true,
  "allOf": [
    {
      "$ref": "digital-letters-data.schema.json"
    }
  ],
  "properties": {
    "arrived-via": {
      "$comment": "Corresponds to ....",
      "description": "How it arrived",
      "enum": [
        "mesh"
      ],
      "type": "string"
    },
    "uri": {
      "$comment": "Corresponds to ....",
      "description": "Where to access the letter",
      "type": "string"
    }
  },
  "required": [
    "arrived-via",
    "uri"
  ],
  "type": "object"
}
